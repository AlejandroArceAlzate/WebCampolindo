@import "nib"
@import "normalize.styl"

a
	text-decoration none 
body
	background #F3E1C7
	font-family 'open sans', Arial, Helvetica, sans-serif
	font-size 12px
footer
	color #2F623C
	marrgin 1em
	padding 1em 
	text-align center
header
	.logoCampolimpio
		position absolute
		height 160px
		margin-left 6em
		width 327px 
		z-index 2 
	.facebook
		height 58px
		position absolute
		margin 1em 5em 0 0
		right 5em
		width 59px
		z-index 4
	.twitter
		height 58px
		position absolute
		margin 1em 5em 0 0
		right .1em
		width 58px
		z-index 5
nav
	padding-left 7em
	ul
		list-style none
		li
			display inline-block
			
section
	iframe
		margin-top 10em
		padding-left 13em
.socios1
	background rgba(white,.4)
	border-top: 2px solid #2F623C;
	margin-left 2em
	figure
		display inline-block
		margin 1em
		&:hover
			transform scale(1.5,1.5)
			transition 1s
.socios2
	background rgba(white,.4)
	border-bottom: 2px solid #2F623C
	margin-left 2em
	figure
		display inline-block
		margin 1em
		&:hover
			transform scale(1.5,1.5)
			transition 1s
		


	